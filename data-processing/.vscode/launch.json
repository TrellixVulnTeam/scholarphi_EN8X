  "version": "0.2.0",
  "configurations": [
    {
      "name": "Extract Equation Tokens",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/process.py",
      "console": "integratedTerminal",
      "args": ["extract-equation-tokens"],
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "."
      }
    },
    {
      "name": "Colorize Equation Tokens",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/process.py",
      "console": "integratedTerminal",
      "args": ["-v", "colorize-equation-tokens"],
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "."
      }
    },
    {
      "name": "Compile Sources with Colorized Equations",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/process.py",
      "console": "integratedTerminal",
      "args": ["-v", "compile-tex-with-colorized-equations"],
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "."
      }
    },
    {
      "name": "Raster Pages with Colorized Equation Tokens",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/process.py",
      "console": "integratedTerminal",
      "args": ["-v", "raster-pages-with-colorized-equation-tokens"],
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "."
      }
    },
    {
      "name": "Locate Tokens",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/process.py",
      "console": "integratedTerminal",
      "args": ["locate-equation-token-hues"],
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "."
      }
    },
    {
      "name": "Locate Symbols",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/process.py",
      "console": "integratedTerminal",
      "args": ["locate-symbols"],
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "."
      }
    },
    {
      "name": "Upload Citations",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/process.py",
      "console": "integratedTerminal",
      "args": ["upload-citations"],
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": ".",
        "PATH": "/usr/local/bin"
      }
    }
  ]
}
