{
  "manifest_version": 2,
  "name": "Scholar Reader",
  "version": "0.0.1",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["extension/launchApp.js"],
    "persistent": true
  },
  "web_accessible_resources": [
    "build/static/*"
  ]
}
